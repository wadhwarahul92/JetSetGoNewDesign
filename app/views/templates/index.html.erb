<script type="text/ng-template" id="airportsTypeAhead.html">
  <a>
    <img src="" style="height: 50px;" ng-src="{{match.model.city.image}}">
    <span ng-bind-html="match.model.label | uibTypeaheadHighlight:query"></span>
  </a>
</script>

<!-- top header search con start -->
<div class="top-header-search-con">
  <!-- header start -->
  <header>
    <div class="col-md-2 logo-con">
      <img src="http://d3tfanr7troppj.cloudfront.net/static_files/images/000/000/136/original/jsg_logo.png?1460534509" class="img-responsive logo-img" />
    </div>
    <div class="right-header-con col-sm-10">
      <ul>
        <li><a href="#"><i class="fa fa-user" aria-hidden="true"></i> Guest</a></li>
        <li><a href="#">Contact</a></li>
        <li><a href="#">Services</a></li>
        <li><a href="#">Testimonial</a></li>
        <li><a href="#">About</a></li>
      </ul>
    </div>
  </header>
  <!-- header finish -->

  <!-- search con start -->
  <div class="search-main-con">
    <h2 class="col-xs-12">
      <span>Your</span>Private Jet
    </h2>
    <div class="col-xs-12">
      <span class="search-main-form" ng-repeat="activity in ctrl.activities">
        <span class="search-form">
          <span class="search-icon departure-icon"></span>
          <input class="search-textfield text-white" title="Departure Airport"
             ng-disabled="$index+1 < ctrl.activities.length"
             placeholder="Departure"
             autocomplete="off"
             typeahead-editable="false"
             typeahead-select-on-exact="true"
             typeahead-template-url="airportsTypeAhead.html"
             ng-model="activity.departure_airport"
             uib-typeahead="airport as airport.label for airport in ctrl.airports | filter:{label:$viewValue}"
             type="text">
        </span>
        <span class="search-form">
          <span class="search-icon arrival-icon"></span>
          <input class="search-textfield text-white" title="Arrival Airport"
             ng-disabled="$index+1 < ctrl.activities.length"
             placeholder="Arrival"
             autocomplete="off"
             typeahead-editable="false"
             typeahead-select-on-exact="true"
             typeahead-template-url="airportsTypeAhead.html"
             ng-model="activity.arrival_airport"
             uib-typeahead="airport as airport.label for airport in ctrl.airports | filter:{label:$viewValue}"
             type="text">
        </span>
        <span class="search-form">
          <div class="dropdown datetimepicker-holder search-textfield date-textfield">
            <a role="button" data-toggle="dropdown" data-target="#" href="Javascript:void(0)"><i class="fa fa-calendar"></i> {{ctrl.formatTime(activity.start_at)}}</a>
            <ul class="dropdown-menu" role="menu">
              <datetimepicker
                  data-on-set-time="ctrl.onSetTime(newDate, oldDate, $index)"
              data-before-render="ctrl.beforeRenderDate($view, $dates, $leftDate, $upDate, $rightDate, $index)"
              datetimepicker-config="{ dropdownSelector: '.fa-calendar' }"
              ng-model="activity.start_at"></datetimepicker>
            </ul>
          </div>
        </span>
        <span class="search-form">
          <span class="search-icon pax-icon"></span>
          <input class="search-textfield text-white pax-textfield" type="number"
                 ng-disabled="$index+1 < ctrl.activities.length"
                 ng-model="activity.pax"
                 placeholder="Pax">
        </span>
        <a href="javascript:void(0)" class="cross-icon" ng-show="$index > 0" ng-click="ctrl.removeActivity($index)">
          <i class="fa fa-times" aria-hidden="true"></i>
        </a>
      </span>
        <a class="search-btn" ng-click="ctrl.create()" href="">Search</a>
        <a class="add-trip-btn" href="" ng-click="ctrl.addActivity()"><i class="fa fa-plus" aria-hidden="true"></i> Add Trip</a>
    </div>
  </div>
  <!-- search con finish -->

</div>
<!-- top header search con finish -->

