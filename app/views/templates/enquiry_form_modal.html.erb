<div class="row " ng-controller="JetSetYatraEnquiryController as ctrl">
  <div class="col-sm-4"></div>
  <div class="col-xs-12">
    <div class="login-black-bg">
      <a href="#" class="closs-btn" ng-click="$close()"><i class="fa fa-times"></i></a>
      <fieldset class="login-register-main">
        <h5>
          <i class="fa fa-user user-icon-top"></i> Enquiry
        </h5>
        <div class="col-xs-12">
          <form ng-submit="ctrl.create()">
            <div class="field margin-top">
              <input type="text" id="name" placeholder="Name" class="form-control text-field" ng-model="ctrl.name" />
              <i class="fa fa-user form-icon"></i>
            </div>
            <div class="field margin-top">
              <input type="email" id="email" placeholder="Email Id" class="form-control text-field" n ng-model="ctrl.email" />
              <i class="fa fa-unlock-alt form-icon"></i>
            </div>
            <div class="field margin-top">
              <input type="number" id="mobile_no" placeholder="Mobile No." class="form-control text-field" n ng-model="ctrl.mobile_number" />
              <i class="fa fa-unlock-alt form-icon"></i>
            </div>
            <div class="field margin-top">
              <!--<input type="text" id="select_package" placeholder="Select Package" class="form-control text-field" n ng-model="ctrl.package" />-->
              <!--<i class="fa fa-unlock-alt form-icon"></i>-->
              select
            </div>
            <div class="field">
              <input type="submit" class="btn btn-primary proceed-btn margin-top" value="Enquire">
            </div>
          </form>
        </div>
      </fieldset>
    </div>
  </div>
  <div class="clearfix"></div>
</div>