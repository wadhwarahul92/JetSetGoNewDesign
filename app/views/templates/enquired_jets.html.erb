<script type="text/javascript" charset="utf-8">
    $('#myCarousel2').carousel({
        interval: false
    });
    $(document).ready(function(){
        setTimeout(function(){
            $( '#carousel_slider span.glyphicon-chevron-left' ).addClass( 'fa fa-chevron-left fa-4' );
            $( '#carousel_slider span.glyphicon-chevron-right' ).addClass( 'fa fa-chevron-right fa-4');
            $('span.fa').css('display', 'inline-block')
        },3000);
        //    $('span.glyphicon span.glyphicon-chevron-left').replaceWith('.fa .fa-chevron-left .fa-4');
        $(".owl-carousel").owlCarousel({
            loop:true,
            margin:10,
            responsiveClass:true,
            responsive:{
                0:{
                    items:1,
                    nav:true
                },
                600:{
                    items:2,
                    nav:true
                },
                990:{
                    items:3,
                    nav:true
                },
                1100:{
                    items:5,
                    nav:true,
                }
            }
        });
    });
</script>
