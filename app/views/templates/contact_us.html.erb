<!-- contact us content start -->
<div class="contact-us-content">

  <div class="contact-info-main">
    <div class="container">
      <div class="col-md-offset-1 col-md-6 col-sm-6 col-xs-12 contact-main-form margin-top-25">
        <div class="contact-form zoomIn animated">
          <form ng-submit="ctrl.create()">
            <h2 class="text-center">Enquire Now</h2>
            <label for="name">Name</label>
            <input id="name" type="text" class="contact-text gray-color" ng-model="ctrl.name">
            <label for="email">Email Id</label>
            <input id="email" type="email" class="contact-text gray-color" ng-model="ctrl.email">
            <label for="contact">Contact</label>
            <input id="contact" ng-model="ctrl.phone" type="text" class="contact-text gray-color">
            <label for="message">Message</label>
            <textarea id="message" class="contact-area gray-color" ng-model="ctrl.message"></textarea>
            <input type="submit" value="Send" class="btn submit-btn hvr-rectangle-in text-18">
          </form>
        </div>
      </div>
      <div class="col-md-5 col-sm-6 col-xs-12 address-detail">
        <h2 class="margin-top-25">Address</h2>
        <div class="col-sm-12 address-col">
          <div class="col-sm-2"><i class="fa fa-building-o contact-icon"></i></div>
          <div class="col-sm-10">
            <b>JetSetGo Aviation Services Private Limited</b><br/>
              A411, Somdutt Chambers I, <br/>
              Bhikaji Cama Place, New Delhi, 110066 <br/>
              Delhi | Mumbai | Bengaluru | Dubai | New York
          </div>
        </div>
        <div class="col-sm-12 phone-col">
          <h2 class="margin-top-25">Phone</h2>
          <div class="col-sm-2"><i class="fa fa-mobile contact-icon"></i></div>
          <div class="col-sm-10">+91-11-39585858</div>
        </div>
        <div class="col-sm-12 email-col">
          <h2 class="margin-top-25">Email</h2>
          <div class="col-sm-2"><i class="fa fa-envelope-o contact-icon"></i></div>
          <div class="col-sm-10">info@jetsetgo.in</div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- contact us content finish -->