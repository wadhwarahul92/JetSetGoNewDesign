                <a href="#" class="modify-btn" ng-click="ctrl.disable_=!ctrl.disable_">Cancel</a>
              </div>

            </div>
          </div>
        </div>
        <!-- search con finish -->

        <div ng-show="ctrl.disable_">
          <div class="row">
            <span  ng-repeat="search_activity in ctrl.search_activities">
