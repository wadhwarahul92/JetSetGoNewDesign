<div class="row " ng-controller="OrganisationAdminController as ctrl">
  <div class="col-sm-12 col-xs-12 login-logo">
    <a class="logo-img" href="/organisations">
      <img class="" src="http://d3tfanr7troppj.cloudfront.net/static_files/images/000/000/162/original/jsg_logo_footer.png?1460978273" />
    </a>
  </div>
  <div class="col-md-4 col-sm-2"></div>
  <div class="col-md-4 col-sm-8 col-xs-12">
    <div class="login-black-bg">
      <fieldset class="login-register-main">
        <h5>
          <i class="fa fa-user user-icon-top"></i> Create Profile
          <!-- <a class="span-text" href="/organisations/sign_up">Register</a> -->
        </h5>
        <div class="col-xs-12">
          <br/>
          <p>You are admin of <strong><%= @organisation.name %></strong>. You can later add more operators to your organisations and manage their roles</p>
          <form ng-submit="ctrl.create()">
            <div class="field margin-top">
              <!--<label for="first_name">First Name</label>-->
              <input type="text" id="first_name" placeholder="Name" class="form-control text-field" ng-model="ctrl.user.first_name">
              <i class="fa fa-user form-icon"></i>
            </div>
            <!--<div class="field margin-top">-->
              <!--&lt;!&ndash;<label for="first_name">Last Name</label>&ndash;&gt;-->
              <!--<input type="text" id="first_name" placeholder="Last Name" class="form-control text-field" ng-model="ctrl.user.last_name">-->
              <!--<i class="fa fa-user form-icon"></i>-->
            <!--</div>-->
            <div class="field margin-top">
              <!--<label for="first_name">Phone</label>-->
              <span id="plus_91" class="country-code-text form-icon">+91</span>
              <input type="text" id="phone" placeholder="Phone" class="form-control text-field" ng-model="ctrl.user.phone">
            </div>
            <div class="field margin-top">
              <!--<label for="first_name">Email</label>-->
              <input type="email" id="email" placeholder="Email" class="form-control text-field" ng-model="ctrl.user.email">
              <i class="fa fa-envelope form-icon"></i>
            </div>
            <div class="field margin-top">
              <!--<label for="password">Password</label>-->
              <input type="password" id="password" placeholder="Password" class="form-control text-field" ng-model="ctrl.user.password">
              <i class="fa fa-unlock-alt form-icon"></i>
            </div>
            <div class="field">
              <input type="submit" value="Next" class="btn btn-primary proceed-btn margin-top">
            </div>
          </form>
        </div>
      </fieldset>
    </div>
  </div>
  <div class="clearfix"></div>
</div>

