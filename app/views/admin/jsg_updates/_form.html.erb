<%= simple_form_for [:admin, @jsg_update] do |f| %>

    <%= f.input :title, as: :string %>
    <%= f.input :description %>
    <%= f.input :source_url, as: :string %>
    <%= f.input :image_url, as: :string %>
    <%= f.submit 'Save', class: 'btn btn-primary' %>
    <%= link_to('Back', admin_jsg_updates_path, class: 'btn btn-primary') %>
<%- end %>