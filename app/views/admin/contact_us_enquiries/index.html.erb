<div class="admin-main-con">
  <div class='col-xs-12'>
    <h2>Contact us Enquiries</h2>
    <div class="admin_content_body">
      <div class="row">
        <div class="col-sm-12">
          <table class="table table-striped table-data-con">
            <thead>
            <tr class="info">
              <th>ID</th>
              <th>Name</th>
              <th>Email</th>
              <th>Phone</th>
              <th>Message</th>
              <th>Enquiry date</th>
            </tr>
            </thead>
            <tbody>
            <% @contact_us_enquiries.each do |contact_us_enquiry| %>
                <tr>
                  <td><%= contact_us_enquiry.id %></td>
                  <td><%= contact_us_enquiry.name.titleize %></td>
                  <td><%= contact_us_enquiry.email %></td>
                  <td><%= contact_us_enquiry.phone %></td>
                  <td><%= contact_us_enquiry.message %></td>
                  <td><%= contact_us_enquiry.created_at.strftime(time_format) %></td>
                </tr>
            <%- end %>
            </tbody>
          </table>
        </div>
      </div>
      <%= will_paginate @contact_us_enquiries %>
    </div>
  </div>
</div>