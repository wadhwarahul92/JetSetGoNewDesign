<div class="row">
  <div class="col-sm-12">
    <table class="table table-striped">
      <thead>
      <tr class="info">
        <th>ID</th>
        <th>Full name</th>
        <th>Email</th>
        <th>Phone</th>
      </tr>
      </thead>
      <tbody>
      <% @customers.each do |customer| %>
          <tr>
            <td><%= "Customer##{customer.id}"%></td>
            <td><%= customer.full_name.capitalize%></td>
            <td><%= customer.email %></td>
            <td><%= customer.phone %></td>
            <!--<td>-->
              <!--<%#= link_to admin_enquiry_path(enquiry) do %>-->
                  <!--<i class="fa fa-eye"></i>-->
              <!--<%#- end %>-->
            <!--</td>-->
          </tr>
      <%- end %>
      </tbody>
    </table>
  </div>
</div>

<%= will_paginate @customers %>