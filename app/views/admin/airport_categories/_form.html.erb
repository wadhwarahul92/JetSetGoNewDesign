<div class="admin-main-con">
  <div class='col-xs-12'>
    <h2>Add Airport Category</h2>
    <div class="admin_content_body">
      <%= simple_form_for [:admin, @airport_category] do |f| %>
          <div class='col-xs-6'>
            <%= f.error_notification %>
            <%= f.input :name %>
          </div>
          <div class="col-sm-6">

          </div>
          <div class='col-xs-12'>
            <%= f.submit 'Save', class: 'btn btn-success' %>
          </div>
      <%- end %>
    </div>
  </div>
</div>