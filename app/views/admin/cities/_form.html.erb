<%= simple_form_for [:admin, @city] do |f| %>
    <%= f.error_notification %>
    <%= f.input :name %>
    <%= f.input :state %>
    <% if f.object.image.present? %>
        <%= image_tag f.object.image.url(:original) %>
    <%- end %>
    <%= f.input :image %>
    <%= f.submit 'Create', class: 'btn btn-success' %>
<%- end %>