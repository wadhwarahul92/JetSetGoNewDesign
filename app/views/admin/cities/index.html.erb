<div class="row">
  <div class="col-sm-12">
    <div class="btn-group" role="group" aria-label="">
      <%= link_to 'New City', new_admin_city_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
<table class="table table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>State</th>
  </tr>
  </thead>
  <tbody>
  <% @cities.each do |city| %>
  <tr>
    <td><%= city.name %></td>
    <td><%= city.state %></td>
  </tr>
  <%- end %>
  </tbody>
</table>