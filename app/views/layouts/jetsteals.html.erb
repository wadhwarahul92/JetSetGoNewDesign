<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <title>JetSteals</title>
  <%= stylesheet_link_tag    'jetsteals', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'jetsteals', 'data-turbolinks-track' => true %>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= render partial: 'shared/zendesk_widget' %>
</head>
<body>
<%= query_diet_widget if Rails.env.development? %>
<%= render partial: 'jetsteals/shared/header' %>
<div class="container-fluid">
  <%= yield %>
</div>
</body>
</html>